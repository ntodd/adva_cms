<h2><%= t(:'adva.calendar.titles.edit_event') %></h2>
<% split_form_for :calendar_event, @event, :url => admin_calendar_event_path, :html => {:multipart => true, :method => :put, :id => 'content_form'} do |f| -%>
	
	<%= render :partial => "form", :object => f %>
	
	<p class="buttons">
		<%= f.submit t(:'adva.common.save') %>
		<%= link_to t(:'adva.common.cancel'), admin_calendar_events_path %>
	</p>
<% end %>
